import kareltherobot.*;



interface layer {
	public class InterfaceTest extends Robot {

		public InterfaceTest(int street, int avenue, Direction direction,
				int beepers) {
			super(street, avenue, direction, beepers);
		}
		
		public void layBeepers() {
			for(int i = 4; i>0; i--) {
				move();
			}
		}
		
	}
}


