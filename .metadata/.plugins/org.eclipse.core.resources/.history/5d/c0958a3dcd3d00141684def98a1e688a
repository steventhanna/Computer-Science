

public class Harvester extends MileWalker 
{
	// Constructor MEthod
	public Harvester(int st, int ave, Direction direction, int numOfBeepers) 
	{
		super(st, ave, direction, numOfBeepers); 
	}
	
	public void moveAndPick() 
	{
		move(); 
		pickBeeper(); 
	}
	
	public void pickRow()
	{
		moveAndPick(); 
		moveAndPick(); 
		moveAndPick(); 
		moveAndPick(); 
		moveAndPick(); 
		moveAndPick(); 	
	}
	
	public void goAroundRight() {
		move();
		turnRight();
		move();
		turnRight();
	}
	
	

}
